{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Dotknet.Cli/bin/Debug/net6.0/Dotknet.Cli.dll",
      "args": [
        "--project-path",
        "${workspaceFolder}/src/Dotknet.Cli"
      ],
      "cwd": "${workspaceFolder}/src/Dotknet.Cli",
      "console": "internalConsole",
      "stopAtEntry": false
    }
  ]
}